{
    "id": "python",
    "cmd": "python3 -m http.server 80",
    "cpus": 0.0625,
    "mem": 64,
    "disk": 64,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "python:3",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    },
    "instances": 3,
    "uris": [],
    "portIndex": 0,
    "protocol": "TCP",
    "gracePeriodSeconds": 300,
    "intervalSeconds": 60,
    "timeoutSeconds": 20,
    "maxConsecutiveFailures": 3
}